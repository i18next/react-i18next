{
  "env": {
    "development": {
      "presets": [
        [
          "@babel/env",
          {
            "targets": {
              "browsers": ["defaults"]
            }
          }
        ],
        "@babel/react",
        [
          "@babel/typescript",
          {
            "onlyRemoveTypeImports": true
          }
        ]
      ],
      "plugins": ["@babel/plugin-transform-runtime"]
    },
    "ESNext": {
      "presets": [
        [
          "@babel/env",
          {
            "targets": {
              "browsers": ["defaults"]
            },
            "modules": false,
            "useBuiltIns": false
          }
        ],
        "@babel/react",
        [
          "@babel/typescript",
          {
            "onlyRemoveTypeImports": true
          }
        ]
      ],
      "plugins": ["@babel/plugin-transform-runtime"]
    },
    "jsnext": {
      "presets": [
        [
          "@babel/env",
          {
            "targets": {
              "browsers": ["defaults"]
            },
            "modules": false,
            "useBuiltIns": false
          }
        ],
        "@babel/react",
        [
          "@babel/typescript",
          {
            "onlyRemoveTypeImports": true
          }
        ]
      ],
      "plugins": ["@babel/plugin-transform-runtime"]
    }
  },
  "comments": false
}
